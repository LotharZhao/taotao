<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zhao.advert.model.Advert" table="t_advert">
		<id name="adid">
		 	<generator class="native"></generator>
		 </id>
		 <property name="adtype"></property>
		 <property name="adname"></property>
		 <property name="desc" column="addesc"></property>
		 <property name="img"></property>
		 <property name="adprice"></property>
		 <property name="adtime"></property>
		 <property name="pid"></property>
	</class>
</hibernate-mapping>